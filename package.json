{
  "name": "sylion-backend",
  "version": "0.1.0",
  "description": "Backend principal de la plateforme SylionAI - Architecture multi-tenant WhatsApp-first avec IA et RAG",
  "main": "dist/app/server.js",
  "scripts": {
    "dev": "tsx watch src/app/server.ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/app/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "health": "curl http://localhost:3000/health",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose up -d"
  },
  "keywords": [
    "sylion",
    "whatsapp",
    "ai",
    "rag",
    "fastify",
    "multi-tenant",
    "backend",
    "typescript"
  ],
  "author": "SylionTech",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "drizzle-orm": "^0.29.1",
    "postgres": "^3.4.3",
    "@neondatabase/serverless": "^0.7.2",
    "drizzle-kit": "^0.20.6",
    "redis": "^4.6.10",
    "bullmq": "^4.15.0",
    "ioredis": "^5.3.2",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4",
    "@google-cloud/vertexai": "^0.4.0",
    "@google-cloud/storage": "^7.6.0",
    "axios": "^1.6.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "@types/node": "^20.10.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "tsx": {
    "tsconfig": "./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SylionTech/sylion-backend.git"
  },
  "bugs": {
    "url": "https://github.com/SylionTech/sylion-backend/issues"
  },
  "homepage": "https://github.com/SylionTech/sylion-backend#readme"
}